# postgres database password for the postgres user
# -> replace it with a secure password
POSTGRES_PASSWORD=mysecurepassword

# keycloak hostname
# -> set it to the FQDN of your instance
KEYCLOAK_HOSTNAME=chimitheque.mesocentre.uca.fr
# keycloak scheme
# -> do not change this value
KEYCLOAK_SCHEME=https
# keycloak admin email
# -> do not change this value
KEYCLOAK_ADMIN=admin@chimitheque.fr
# keycloak admin password
# -> replace it with a secure password
KEYCLOAK_ADMIN_PASSWORD=anotherpassword
# -> do not change this value
KEYCLOAK_HOSTNAME_STRICT_HTTPS=true

# chimitheque docker image
# -> do not change this value except for upgrades
CHIMITHEQUE_IMAGE=tbellembois/gochimitheque:2.1.0
# chimitheque URL
# -> set it to the URL of your instance
CHIMITHEQUE_APPURL=https://chimitheque.mesocentre.uca.fr
# chimitheque debug mode
# -> for debug purposes
CHIMITHEQUE_DEBUG=false
# chimitheque OIDC client secret
# -> replace it with a secure password
CHIMITHEQUE_OIDCCLIENTSECRET=mysupersecret
# chimitheque admin list
# -> comma separated list of emails - do not remove admin@chimitheque.fr
CHIMITHEQUE_ADMINS=admin@chimitheque.fr,bar@foo.com
